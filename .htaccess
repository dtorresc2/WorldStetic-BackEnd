RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

#Ruta para usuarios
RewriteRule ^usuarios/([0-9]+)$ routes/usuarios.php?id=$1 [QSA,NC,L]
RewriteRule usuarios/auth routes/autenticacion.php? [QSA,NC,L]
RewriteRule usuarios routes/usuarios.php [QSA,NC,L]

#Ruta para servicios
RewriteRule ^servicios/([0-9]+)$ routes/servicios.php?id=$1 [QSA,NC,L]
#RewriteRule ^servicios/(.*)$ routes/servicios.php?id=$1 [QSA,NC,L]
RewriteRule servicios routes/servicios.php [QSA,NC,L]